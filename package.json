{
  "name": "soko-landig-page",
  "version": "1.0.0",
  "description": "This is the landing page for SOKo - Subject Oriented Communications.",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:copy && npm run build:client && npm run build:lambda",
    "build:copy": "copyfiles src/assets/* src/assets/documents/* src/assets/imgs/* dist/ -u 2",
    "build:client": "webpack --config webpack.prod.js",
    "build:lambda": "netlify-lambda build src/netlify-functions",
    "formatHtml": "prettyhtml src/assets/**.html",
    "start:client": "webpack-dev-server --open --config webpack.dev.js",
    "start:lambda": "netlify-lambda serve src/netlify-functions -c webpack.server.js",
    "test": "tape 'src/**/*Test.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xaverfleer/sokoLandigPage.git"
  },
  "keywords": [],
  "author": "Xaver Fleer",
  "private": true,
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/xaverfleer/sokoLandigPage/issues"
  },
  "homepage": "https://github.com/xaverfleer/sokoLandigPage#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.10.2",
    "@starptech/prettyhtml": "^0.10.0",
    "babel-loader": "^8.1.0",
    "browserslist": "^4.12.0",
    "copyfiles": "^2.3.0",
    "css-loader": "^3.5.3",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.21.1",
    "express": "^4.17.1",
    "file-loader": "^6.0.0",
    "netlify-cli": "^2.59.1",
    "netlify-lambda": "^1.6.3",
    "prettier": "^1.19.1",
    "style-loader": "^1.2.1",
    "tape": "^5.0.1",
    "terser-webpack-plugin": "^3.0.3",
    "vue-loader": "^15.9.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@sendgrid/mail": "^6.5.5",
    "crypto": "^1.0.1",
    "encoding": "^0.1.12",
    "faunadb": "^2.14.2",
    "vue": "^2.6.11",
    "vue-router": "^3.3.2"
  }
}
